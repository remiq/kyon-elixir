<div class="list">
  <%= for item <- @items do %>
  <div class="item">
    <a href="<%= item_path(@conn, :view, item.id, "_")  %>">
      <img src="/thumb/<%= prefix %><%= item.id %>.jpg" />
    </a>
  </div>
  <% end %>
</div>
<%= render Placebooru.SharedView, "pager.html",
  page: @page, url: &(activity_path(@conn, :favs_all, &1)) %>
